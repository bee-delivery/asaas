{
    "name": "luanrodrigues/asaas",
    "description": "A package to php integration with Asaas",
    "keywords": ["asaas", "laravel", "php"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Luan Rodrigues",
            "email": "luan@luanrodrigues.com.br"
        }
    ],
    "minimum-stability": "dev",
    "require": {},
